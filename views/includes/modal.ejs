<script>
    function alertModal(text) {
        Swal.fire(
            '',
            text,
            'success'
        )
    }

    function refreshModal(text) {
        Swal.fire({
            text: text,
        }).then((result) => {
            if (result.isConfirmed) {
                location.reload();
            }
        })
    }
</script>